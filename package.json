{
  "name": "@wasmer/io-devices-lib-assemblyscript",
  "version": "0.0.1",
  "description": "Library for interacting with the Wasmer Experimental IO Devices in AssemblyScript",
  "main": "assemblyscript/lib/lib.ts",
  "scripts": {
    "start": "../wasmer/target/release/wasmer run --enable-experimental-framebuffer assemblyscript/debug-example/build/optimized.wasm",
    "start:quick-start": "../wasmer/target/release/wasmer run --enable-experimental-framebuffer assemblyscript/quick-start/build/optimized.wasm",
    "build": "npm run asbuild",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized && npm run asbuild:quickstart",
    "asbuild:untouched": "asc assemblyscript/debug-example/index.ts -b assemblyscript/debug-example/build/untouched.wasm -t assemblyscript/debug-example/build/untouched.wat --sourceMap --validate --debug --use abort=wasi_abort --runtime stub",
    "asbuild:optimized": "asc assemblyscript/debug-example/index.ts -b assemblyscript/debug-example/build/optimized.wasm -t assemblyscript/debug-example/build/optimized.wat --sourceMap --validate --optimize --use abort=wasi_abort --runtime stub",
    "asbuild:quick-start": "asc assemblyscript/quick-start/graphics.ts -b assemblyscript/quick-start/build/optimized.wasm -t assemblyscript/quick-start/build/optimized.wat --sourceMap --validate --optimize --use abort=wasi_abort --runtime stub"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wasmerio/io-devices-lib.git"
  },
  "keywords": [
    "assemblyscript",
    "wasmer",
    "graphics",
    "audio",
    "input",
    "wasi",
    "wasm",
    "web",
    "assembly",
    "webassembly"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wasmerio/io-devices-lib/issues"
  },
  "homepage": "https://github.com/wasmerio/io-devices-lib#readme",
  "dependencies": {
    "as-wasi": "git+https://github.com/jedisct1/as-wasi.git"
  }
}
